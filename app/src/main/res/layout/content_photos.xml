<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="16dp"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.crisiscore.www.intexsofttestproject.PhotosActivity"
    tools:showIn="@layout/activity_photos">

    <fragment
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/photosFragment"
        android:name="com.crisiscore.www.intexsofttestproject.PhotosFragment"
        tools:layout="@layout/photos_fragment"/>

</LinearLayout>
